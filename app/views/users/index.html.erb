<% @users.each do |user| %>
  <div class="jumbotron">
    <p> <%= user.name %> </p>
    <p> <%= user.email %> </p>
    <p>Admin = <%= user.admin %> </p>
    <p>Create =  <%= user.created_at %> </p>
    <p> <%= link_to "Delete", user, method: :delete %> </p>
    <p> <%= link_to "Edit", '#' %> </p>
  </div>
<% end %>
