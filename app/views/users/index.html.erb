<% @users.each do |user| %>
  <div class="col-md-6 col-md-offset-3">
    <hr>
    <p> <%= user.name %> </p>
    <p> <%= user.email %> </p>
    <p>Admin = <%= user.admin %> </p>
    <p>Create =  <%= user.created_at %> </p>
    <p> <%= link_to "Delete", user, method: :delete %> </p>
    <p> <%= link_to "Edit", edit_user_path(user)%> </p>
  </div>
<% end %>
